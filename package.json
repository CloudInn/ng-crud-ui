{
    "name": "ng-crud-ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy_conf.json",
        "build": "ng build",
        "test": "ng test",
        "lint": " ng lint --type-check",
        "e2e": "ng e2e",
        "build-crud": "ng build crud",
        "publish-crud": "cd dist/crud && npm publish && cd ../../",
        "build-docs": "ng build --prod docs --output-path ./docs --base-href /ng-crud-ui/ --deploy-url /ng-crud-ui/"
    },
    "private": true,
    "main": "./dist/crud/fesm2022/ng-crud-ui.mjs",
    "typings": "./dist/crud/index.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./dist/crud/index.d.ts",
            "default": "./dist/crud/fesm2022/ng-crud-ui.mjs"
        }
    },
    "dependencies": {
        "@angular/animations": "^20.0.0",
        "@angular/cdk": "^20.0.0",
        "@angular/common": "^20.0.0",
        "@angular/compiler": "^20.0.0",
        "@angular/core": "^20.0.0",
        "@angular/flex-layout": "^15.0.0-beta.42",
        "@angular/forms": "^20.0.0",
        "@angular/material": "^20.0.0",
        "@angular/material-moment-adapter": "^20.0.0",
        "@angular/platform-browser": "^20.0.0",
        "@angular/platform-browser-dynamic": "^20.0.0",
        "@angular/router": "^20.0.0",
        "@jonijnm/amazing-time-picker": "^2.2.0",
        "@ngx-translate/core": "^15.0.0",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "mat-select-autocomplete-angular19": "^1.5.39",
        "moment": "^2.30.1",
        "ngx-cookie-service": "^20.1.0",
        "ngx-markdown": "^20.0.0",
        "rxjs": "^7.8.0",
        "tslib": "^2.0.0",
        "zone.js": "^0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^20.2.0",
        "@angular/cli": "^20.2.0",
        "@angular/compiler-cli": "^20.0.0",
        "@angular/language-service": "^20.0.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "eslint": "^9.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^20.0.0",
        "protractor": "~7.0.0",
        "sass": "^1.83.0",
        "ts-node": "~10.9.0",
        "typescript": "~5.8.0"
    }
}
